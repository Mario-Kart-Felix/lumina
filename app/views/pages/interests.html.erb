<div class="container d-flex justify-content-center">
  <div class="cards">
   <!--  <form action="/feed"> -->
    <% interests.each_with_index do |interest, index| %>
    <div class="m-3 text-center">
      <label class="image-checkbox" data-name="<%= interest %>">
          <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= interest_images_url[index] %>)">
            <%= interest %>
          </div>

      </label>
    </div>
      <!-- </form> -->
    <%end %>
    </div>
  </div>
  <div class="text-center">
    <%= link_to "Submit", interests_path, method: :post, id: "next-page", class:"btn btn-primary m-5" %>
  </div>
</div>
